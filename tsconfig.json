{
	"compilerOptions": {
		"module": "CommonJS",
		"target": "ES5",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
		"inlineSources": true,
		"resolveJsonModule": true,
		"outDir": "./bin",
		"declaration": true
	},
	"include": ["src/**/*.ts"]
}
